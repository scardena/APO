							button.btn.btn-info.btn-sm(data-widget='collapse',data-toggle='tooltip',title='Collapse')
								i.fa.fa-minus

						h3.box-title TablesSpaces SCO
					.box-body.table-responsive
						table.table.table-bordered.table-hover
							thead
								tr
									th Name
									th MB Used
									th MB Free
									th MB Total
									th MB Not Alloc
									th Free %
									th Datafiles
									th Status
								tbody
								each table, i in tablespacessco
									td #{table.name}
									td #{table.mbused}
									td #{table.mbfree}
									td #{table.mbtotal}
									td #{table.mbnotall}
									td #{table.percused}
									td #{table.datafiles}
									td 
										- var status = table.status	
										if status == "ONLINE"
											span.label.label-success Online
										else
											span.label-label-danger Danger
									tr

