extends layout
block content
	// Content Header (Page header)
	section.content-header
		h1
			| Data Ingested
			small Control panel
		ol.breadcrumb
			li
				a(href='#')
					i.fa.fa-dashboard
					| Home
			li.active Dashboard
	// Main content
	section.content

		.row
			section.col-lg-12.connectedSortable
				.box.box-solid.box-info
					.box-header
						.pull-right.box-tools
							button.btn.btn-info.btn-sm(data-widget='collapse',data-toggle='tooltip',title='Collapse')
								i.fa.fa-minus
						h3.box-title Data ingested last 30 days
					.box-body.chart-responsive
						#bar-chart1.chart(style='height: 400px;')
	
		.row
			section.col-lg-12.connectedSortable
				.box.box-solid.box-info
					.box-header
						.pull-right.box-tools
							button.btn.btn-info.btn-sm(data-widget='collapse',data-toggle='tooltip',title='Collapse')
								i.fa.fa-minus
						h3.box-title Data ingested last 12 Months
					.box-body.chart-responsive
						#bar-chart2.chart(style='height: 400px;')
	
		.row 
			section.col-lg-12.connectedSortable
				.box.box-solid.box-info
					.box-header
						.pull-right.box-tools
							button.btn.btn-info.btn-sm(data-widget='collapse',data-toggle='tooltip',title='Collapse')
								i.fa.fa-minus
						h3.box-title ASDMs ingested last 30 days
					.box-body.chart-responsive
						#bar-chart3.chart(style='height: 400px;')


		.row
			section.col-lg-12.connectedSortable
				.box.box-solid.box-info
					.box-header
						.pull-right.box-tools
							button.btn.btn-info.btn-sm(data-widget='collapse',data-toggle='tooltip',title='Collapse')
								i.fa.fa-minus
						h3.box-title ASDMs ingested last 12 months
					.box-body.chart-responsive
						#bar-chart4.chart(style='heigth: 400px;')

		.row
			section.col-lg-6.connectedSortable
				.box.box-solid.box-info
					.box-header
						.pull-right.box-tools
							button.btn.btn-info.btn-sm(data-widget='collapse',data-toggle='tooltip',title='Collapse')
								i.fa.fa-minus
						h3.box-title Ingested data SCO
					.box-body.table-responsive
						table.table.table-bordered.table-hover
							thead
								tr
									th Date
									th Files
									th TAR	
									th Size (in MB)
								tbody
								each metric, i in res5
									td #{metric.date}
									td #{metric.files}
									td #{metric.tar}
									td #{metric.size}
									tr


			section.col-lg-6.connectedSortable
				.box.box-solid.box-info
					.box-header
						.pull-right.box-tools
							button.btn.btn-info.btn-sm(data-widget='collapse',data-toggle='tooltip',title='Collapse')
								i.fa.fa-minus
						h3.box-title Ingested data OSF
					.box-body.table-responsive
						table.table.table-bordered.table-hover
							thead
								tr
									th Date
									th Files
									th Size (in MB)
								tbody
								each metric, i in res6
									td #{metric.date}
									td #{metric.files}
									td #{metric.size}
									tr





