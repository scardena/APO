			.col-md-6
				// Custom Tabs
				.nav-tabs-custom
					ul.nav.nav-tabs
						li.active
							a(href='#tab_1', data-toggle='tab') Tab 1
						li
							a(href='#tab_2', data-toggle='tab') Tab 2
