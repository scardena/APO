extends ../layout
block content
	// Content Header (Page header)
	section.content-header
		h1
			| General UI
			small Preview of UI elements
		ol.breadcrumb
			li
				a(href='#')
					i.fa.fa-dashboard
					| Home
			li.active Blank page
	// Main content
	section.content
		// START ALERTS AND CALLOUTS
		h2.page-header Alerts and Callouts
		.row
			.col-md-6
				.box.box-danger
					.box-header
						i.fa.fa-warning
						h3.box-title Alerts
					// /.box-header
					.box-body
						.alert.alert-danger.alert-dismissable
							i.fa.fa-ban
							button.close(type='button', data-dismiss='alert', aria-hidden='true') &times;
							b Alert!
							| Danger alert preview. This alert is dismissable. A wonderful serenity has taken possession of my entire soul, like these sweet mornings of spring which I enjoy with my whole heart.
						.alert.alert-info.alert-dismissable
							i.fa.fa-info
							button.close(type='button', data-dismiss='alert', aria-hidden='true') &times;
							b Alert!
							| Info alert preview. This alert is dismissable.
						.alert.alert-warning.alert-dismissable
							i.fa.fa-warning
							button.close(type='button', data-dismiss='alert', aria-hidden='true') &times;
							b Alert!
							| Warning alert preview. This alert is dismissable.
						.alert.alert-success.alert-dismissable
							i.fa.fa-check
							button.close(type='button', data-dismiss='alert', aria-hidden='true') &times;
							b Alert!
							| Success alert preview. This alert is dismissable.
					// /.box-body
				// /.box
			// /.col
			.col-md-6
				.box.box-info
					.box-header
						i.fa.fa-bullhorn
						h3.box-title Callouts
					// /.box-header
					.box-body
						.callout.callout-danger
							h4 I am a danger callout!
							p
								| There is a problem that we need to fix. A wonderful serenity has taken possession of my entire soul, like these sweet mornings of spring which I enjoy with my whole heart.
						.callout.callout-info
							h4 I am an info callout!
							p Follow the steps to continue to payment.
						.callout.callout-warning
							h4 I am a warning callout!
							p This is a yellow callout.
					// /.box-body
				// /.box
			// /.col
		// /.row
		// END ALERTS AND CALLOUTS
		// START CUSTOM TABS
		h2.page-header AdminLTE Custom Tabs
