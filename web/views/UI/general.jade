							.progress-bar.progress-bar-primary(role='progressbar', aria-valuenow='40', aria-valuemin='0', aria-valuemax='100', style='width: 40%')
								span.sr-only 40% Complete (success)
						| Class:
						code .sm
						.progress.sm.progress-striped.active
							.progress-bar.progress-bar-success(role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100', style='width: 20%')
								span.sr-only 20% Complete
						| Class:
						code .xs
						.progress.xs.progress-striped
							.progress-bar.progress-bar-warning(role='progressbar', aria-valuenow='60', aria-valuemin='0', aria-valuemax='100', style='width: 60%')
								span.sr-only 60% Complete (warning)
					// /.box-body
				// /.box
			// /.col (left)
			.col-md-6
				.box.box-solid
					.box-header
						h3.box-title Progress bars
					// /.box-header
					.box-body
